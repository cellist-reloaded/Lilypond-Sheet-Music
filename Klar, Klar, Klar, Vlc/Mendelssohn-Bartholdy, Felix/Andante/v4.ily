vd = \relative c, {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    \partial 2 e4 fis
    gis a cis h8 a
    a2 gis4 cis
    a gis8 fis h2
    e, \breathe e4 fis
    gis e h'2

    cis cis4 dis8 e
    dis4 dis, e fis
    h2 \breathe
  }

  \repeat volta 2 {
    h4 a
    gis2 gis4 fis
    eis2 fis4 cis'

    fis, gis a2
    gis \breathe cis
    h a~
    a \breathe gis4 cis
    a gis8 fis h4 h
    e,?2
  }
}