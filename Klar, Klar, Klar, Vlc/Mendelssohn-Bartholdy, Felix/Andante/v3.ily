vc = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    \partial 2 h2~
    h4 a e'2
    dis h4 cis~
    cis2 h~
    h \breathe h~
    h4 h e dis~

    dis e fis2~
    fis gis4 fis
    fis2 \breathe
  }

  \repeat volta 2 {
    dis
    e eis4 fis
    gis2 cis,4 eis

    cis2. cis4
    c2 \breathe e,4 fis
    gis2 fis~
    fis \breathe h4 cis?~
    cis2 h
    gis
  }
}