va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    c4 h16( c d h) c4
    e d16( e f d) e4
    g8( e) e-. e-. e-. e-.
    f( d) d-. d-. d-. d-.
    c4 h16( c d h) c4
    e d16( e f d) e4

    a8 a16( f) f8 f16( e) d8 d16( c)
    h( c d h) c4 c
  }

  \repeat volta 2 {
    g'8( e) e-. e-. e-. e-.
    f( d) d-. d-. d-. d-.
    e( c) c-. c-. c-. c-.
    d( h) h-. h-. h-. h-.

    c4 h16( c d h) c4
    e d16( e f d) e4
    a8 a16( g) f8 f16( e) d8 d16( c)
    h( c d h) c4 c
  }
}  