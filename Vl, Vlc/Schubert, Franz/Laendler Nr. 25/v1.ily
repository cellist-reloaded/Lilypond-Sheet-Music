    h4 a8 h g d
    h4 e8 d g,4
    c' h8 c a d,
    c4 e8 d fis,4

    h' a8 h g fis
    e4 fis8 e c a
    fis a cis d d' fis,
    g2 r4
  }
  
  \repeat volta 2 {
    fis,8 c'? d r a' d,
    fis, c' d r a' d,
