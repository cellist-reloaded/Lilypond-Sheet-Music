va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    h4\p c d
    g, a h
    a8( h) c a h g

    a( g) a h c a
    h4 c d
    g, a h
    a8( h) c( h) a( h)
    g2.
  }

  \repeat volta 2 {
    h8( a g a) h g
    d'2.
    h8( a g a) h g
    d'2.

    h8( a h) c a h
    c( h c) d h c
    a( h c) h a h
    g2.
  }
}