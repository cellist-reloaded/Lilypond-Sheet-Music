vb = \relative c {
  \voiceconsts
  \clef "bass"
  
  \repeat volta 2 {
    g8(\p d') g( d)
    g,(d') h( d)
    a'( g) d4
    g8( d) h4
    g8( d') g( d)

    g,( d') h( d)
    a'( g) d4
    h8( d) g r
  }
  \repeat volta 2 {
    c,[( d c d)]
    c[( d c d)]
    h( d) g,( h)

    d( fis) a( d)
    c,[( d) c( d)]
    c[( d) c( d)]
    h( d) g,( h)
    d4 r
    g,8( d') g( d)
    g,( d') h( d)

    a'( g) d4
    g8( d) h4
    g8( d') g( d)
    g,( d') h( d)
    a'( g) d4
    h8( d) g4
  }
}