va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    \partial 4 a4\mf
    d8 a a a b a a a
    g a f a e a g a
    f a e a d,4 \breathe a'\p
    d8 a a a b a a a

    g a f a e a d, a'
    cis, a cis e a4 \breathe c!8\mf a
    g( f) f2 g8 a
    c( b) b2 c8 d
    d( e,) e4. c8 d e

    f\< g a b c d\! c d
    c d\> c b a g f e\!
    f\p g\< a b c d\! c d
    c d\> c b a g f e\!
    f4 c f,
  }

  \repeat volta 2 {
    c''\mf
    a8 c c c g c c c
    a f e f g a b c
    a\p c c c g c c c
    a f\cresc g a b a g f
    e e f g a g f e

    d e f d a4 \breathe a'\mf
    d8 a a a b a a a
    g a f a e a g a
    f a e a d,4 \breathe a'\p
    d8 a a a b a a a

    g a f a e a g a
    f\< g a b a d\! cis d
    a d\> c! b a g f e\!
    d\< e f g a d\! cis d
    a d\> c! b a g f e\!
    d4 a d,
  }
}