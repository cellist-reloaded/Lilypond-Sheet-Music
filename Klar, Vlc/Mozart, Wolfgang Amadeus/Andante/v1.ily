va = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    d2\f e8( d e fis)
    d4 a a a
    e'2 fis8( e fis g)
    e4 a, a a
    a'2 cis,8( d e cis)

    d2 h'8( gis) e d
    cis a'( fis h) a4 gis
    gis2( a4) r
  }

  \repeat volta 2 {
    a2\mf fis8( g! a h)
    g2 e8( fis g a)

    g( fis e fis) d4 g
    fis2( e4) r
    g2\p fis8( e d cis)
    a'2 g8( fis e d)
    h( h' g e) d4 cis
    cis2( d4) r
  }
}